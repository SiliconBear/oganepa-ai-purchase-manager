{
  "name": "oganepa-ai-purchase-manager",
  "version": "1.0.0",
  "description": "This is the artificial intelligent application that handles purchases via whatsapp messaging using twilio-and-dialogflow integration.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "lint": "eslint . --max-warnings 0 --no-error-on-unmatched-pattern --ext ts,js",
    "prettier": "prettier --write . --ignore-unknown",
    "test": "npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write . --ignore-unknown",
    "*.{ts,js}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SiliconBear/oganepa-ai-purchase-manager.git"
  },
  "keywords": [
    "Artificial",
    "Intelligence",
    "Twilio",
    "Dialogflow",
    "AI"
  ],
  "author": "Siliconbear",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SiliconBear/oganepa-ai-purchase-manager/issues"
  },
  "homepage": "https://github.com/SiliconBear/oganepa-ai-purchase-manager#readme",
  "dependencies": {
    "dialogflow": "^1.2.0",
    "dotenv": "^8.2.0",
    "joi": "^17.3.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "pb-util": "^0.1.3",
    "realm": "^10.1.2",
    "twilio": "^3.54.1"
  },
  "devDependencies": {
    "@types/dialogflow": "^4.0.4",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.14.19",
    "@types/realm": "^1.13.0",
    "@types/twilio": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "eslint-config-koa": "^2.0.2",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^5.0.6",
    "lint-staged": "^10.5.3",
    "node-pre-gyp": "^0.17.0",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
